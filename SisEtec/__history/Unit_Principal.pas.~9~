begin
  FRM_Principal.Hide;
  FRM_Cadastro_Curso.Show;
end;

procedure TFRM_Principal.Image19MouseDown(Sender: TObject; Button: TMouseButton;
  Shift: TShiftState; X, Y: Single);
begin
  ShadowEffect15.Opacity := 1;
end;

procedure TFRM_Principal.Image19MouseEnter(Sender: TObject);
begin
  ShadowEffect15.Enabled := true;
end;

procedure TFRM_Principal.Image19MouseLeave(Sender: TObject);
begin
  ShadowEffect15.Enabled := false;
end;

procedure TFRM_Principal.Image19MouseUp(Sender: TObject; Button: TMouseButton;
  Shift: TShiftState; X, Y: Single);
begin
  ShadowEffect15.Opacity := 0.6;
end;

procedure TFRM_Principal.Image21Click(Sender: TObject);
begin
  FRM_Principal.Hide;
  FRM_Cadastro_Turma.Show;
end;

procedure TFRM_Principal.Image21MouseDown(Sender: TObject; Button: TMouseButton;
  Shift: TShiftState; X, Y: Single);
begin
  ShadowEffect16.Opacity := 1;
end;

