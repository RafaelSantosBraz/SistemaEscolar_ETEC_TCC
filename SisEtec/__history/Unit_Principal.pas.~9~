begin
  ShadowEffect16.Enabled := true;
end;

procedure TFRM_Principal.Image21MouseLeave(Sender: TObject);
begin
  ShadowEffect16.Enabled := false;
end;

procedure TFRM_Principal.Image21MouseUp(Sender: TObject; Button: TMouseButton;
  Shift: TShiftState; X, Y: Single);
begin
  ShadowEffect16.Opacity := 0.6;
end;

