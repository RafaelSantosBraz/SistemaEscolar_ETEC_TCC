var
  FRM_Principal: TFRM_Principal;
  Estado_Menu_Opcoes, Estado_Menu_Usuarios, Estado_Menu_Ferramentas: Boolean;
  Num_Painel, Num_Painel_Auxiliar: Integer;

implementation

{$R *.fmx}

uses Unit_Mensagens, Unit_Controle, Unit_Cadastro_Professor, Unit_Login,
  Unit_Cadastro_Administrador, Unit_Cadastro_Coordenador, Unit_Arquivos,
  UnitAluno, Unit_Cadastro_Curso, Unit_Cadastro_Disciplina,
  Unit_Cadastro_Turmas, Unit_Agenda, Unit_Pesquisa;

function TFRM_Principal.Fechar_Menus(): Boolean;
begin
  if Estado_Menu_Opcoes = true then
  begin
    Num_Painel_Auxiliar := 1;
  end;
  if Estado_Menu_Usuarios = true then
  begin
    Num_Painel_Auxiliar := 2;
  end;
  if Estado_Menu_Ferramentas = true then
  begin
    Num_Painel_Auxiliar := 3;
  end;
  if (Estado_Menu_Opcoes = false) and (Estado_Menu_Usuarios = false) and
    (Estado_Menu_Ferramentas = false) then
  begin
    Result := false;
  end
  else
  begin
    Result := true;
  end;
end;

procedure TFRM_Principal.Button1Click(Sender: TObject);
begin
  Estado_Menu_Opcoes := false;
  Panel3.Visible := true;
  Timer2.Enabled := true;
end;

procedure TFRM_Principal.Button2Click(Sender: TObject);
begin
  Estado_Menu_Opcoes := true;
  Timer2.Enabled := true;
end;

procedure TFRM_Principal.FormCloseQuery(Sender: TObject; var CanClose: Boolean);
begin
  Mensagens.Encerrar_Aplicacao;
  CanClose := false;
end;

procedure TFRM_Principal.FormShow(Sender: TObject);
begin
  Centro_Controle.Reset_Menus;
end;

