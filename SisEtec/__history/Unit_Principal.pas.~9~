procedure TFRM_Principal.Image2Click(Sender: TObject);
begin
  FRM_Principal.Hide;
end;

procedure TFRM_Principal.Panel2Click(Sender: TObject);
begin
  if Fechar_Menus = true then
  begin
    Num_Painel := Num_Painel_Auxiliar;
    Timer2.Enabled := true;
  end;
end;

procedure TFRM_Principal.Panel6Click(Sender: TObject);
begin
  if Fechar_Menus = true then
  begin
    Num_Painel := Num_Painel_Auxiliar;
    Timer2.Enabled := true;
  end;
end;

procedure TFRM_Principal.StatusBar1Click(Sender: TObject);
begin
  if Fechar_Menus = true then
  begin
    Num_Painel := Num_Painel_Auxiliar;
    Timer2.Enabled := true;
  end;
end;

procedure TFRM_Principal.Timer2Timer(Sender: TObject);
begin
  if Num_Painel = 1 then
  begin
    if Estado_Menu_Opcoes = false then
    begin
      Panel3.Visible := true;
      if Panel3.Height < 200 then
      begin
        Panel3.Height := Panel3.Height + 4;
      end;
      if Panel3.Width < 200 then
      begin
        Panel3.Width := Panel3.Width + 4;
      end
      else
      begin
        Estado_Menu_Opcoes := true;
        Timer2.Enabled := false;
      end;
    end
    else
    begin
      if Panel3.Height > 1 then
      begin
        Panel3.Height := Panel3.Height - 4;
      end;
      if Panel3.Width > 1 then
      begin
        Panel3.Width := Panel3.Width - 4;
      end;
      if (Panel3.Height = 1) and (Panel3.Width = 1) then
      begin
        Panel3.Visible := false;
        Estado_Menu_Opcoes := false;
        Timer2.Enabled := false;
      end;
    end;
  end;
  if Num_Painel = 2 then
  begin
    if Estado_Menu_Usuarios = false then
    begin
      Panel4.Visible := true;
      if Panel4.Height < 400 then
      begin
        Panel4.Height := Panel4.Height + 4;
      end
      else
      begin
        Estado_Menu_Usuarios := true;
        Timer2.Enabled := false;
      end;
      if Panel4.Width < 200 then
      begin
        Panel4.Width := Panel4.Width + 4;
      end;
    end
    else
    begin
      if Panel4.Height > 1 then
      begin
        Panel4.Height := Panel4.Height - 4;
      end;
      if Panel4.Width > 1 then
      begin
        Panel4.Width := Panel4.Width - 4;
      end;
      if (Panel4.Height = 1) and (Panel4.Width = 1) then
      begin
        Panel4.Visible := false;
        Estado_Menu_Usuarios := false;
        Timer2.Enabled := false;
      end;
    end;
  end;
  if Num_Painel = 3 then
  begin
    if Estado_Menu_Ferramentas = false then
    begin
      Panel5.Visible := true;
      if Panel5.Height < 200 then
      begin
        Panel5.Height := Panel5.Height + 4;
      end;
      if Panel5.Width < 200 then
      begin
        Panel5.Width := Panel5.Width + 4;
      end
      else
      begin
        Estado_Menu_Ferramentas := true;
        Timer2.Enabled := false;
      end;
    end
    else
    begin
      if Panel5.Height > 1 then
      begin
        Panel5.Height := Panel5.Height - 4;
      end;
      if Panel5.Width > 1 then
      begin
        Panel5.Width := Panel5.Width - 4;
      end;
      if (Panel5.Height = 1) and (Panel5.Width = 1) then
      begin
        Panel5.Visible := false;
        Estado_Menu_Ferramentas := false;
        Timer2.Enabled := false;
      end;
    end;
  end;
end;

end.
